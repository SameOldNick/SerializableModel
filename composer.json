{
    "name": "little-apps/serializable-model",
    "description": "Simple package for serializable columns in a Laravel model.",
    "require": {
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "orchestra/testbench": "~3.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
	"autoload": {
		"psr-4": {
			"LittleApps\\SerializableModel\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/models"
		]
	},
